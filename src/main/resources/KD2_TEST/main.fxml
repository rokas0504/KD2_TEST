<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.fxControllers.Main">
    <children>
        <AnchorPane fx:id="darbuotojasManagerPane" minHeight="0.0" minWidth="0.0" prefHeight="520.0" prefWidth="888.0">
            <children>
                <ListView fx:id="darbuotojasListField" layoutX="26.0" layoutY="24.0" onMouseClicked="#loadDarbuotojasData" prefHeight="468.0" prefWidth="400.0" />
                <TextField fx:id="nameField" layoutX="442.0" layoutY="128.0" prefHeight="29.0" prefWidth="169.0" promptText="Enter name" />
                <TextField fx:id="surnameField" layoutX="442.0" layoutY="181.0" prefHeight="29.0" prefWidth="169.0" promptText="Enter username" />
                <Button layoutX="442.0" layoutY="393.0" mnemonicParsing="false" onAction="#createNewDarbuotojas" text="Add" />
                <Button layoutX="491.0" layoutY="393.0" mnemonicParsing="false" onAction="#openWeekDayChooser" text="Choose working Days" />
                <Button layoutX="642.0" layoutY="393.0" mnemonicParsing="false" onAction="#slowToggle" text="Slow toggle" />
                <Button layoutX="739.0" layoutY="393.0" mnemonicParsing="false" onAction="#resetAllFields" text="Clear fields" />
                <Label fx:id="slowToggleLabelTrue" layoutX="614.0" layoutY="365.0" text="Slow input is selected" visible="false" />
                <Label fx:id="slowToggleLabelFalse" layoutX="614.0" layoutY="365.0" text="Slow input is not selected" visible="true" />

            </children>
        </AnchorPane>
    </children>

</AnchorPane>
